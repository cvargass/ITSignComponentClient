    <script async defer type="module" src="https://fortifyapp.com/external/webcomponent/build/peculiar.esm.js"></script>
    <script async defer nomodule src="https://fortifyapp.com/external/webcomponent/build/peculiar.js"></script>
    <link rel="stylesheet" href="https://fortifyapp.com/external/webcomponent/build/peculiar.css">
    <!-- Crypto -->
    <script src="https://fortifyapp.com/external/pvtsutils/pvtsutils.js"></script>
    <script src="https://fortifyapp.com/external/asn1js/asn1.min.js"></script>
    <script src="https://fortifyapp.com/external/pkijs/pki.min.js"></script>

    <!-- Fetch Polyfill -->
    <script defer nomodule src="https://fortifyapp.com/external/whatwg-fetch@3.0.0/fetch.umd.js"></script>

